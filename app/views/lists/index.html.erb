<h1 class="text-center">Name</h1>
<div class="d-flex justify-content-center">
  <ul>
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %></li>
  <% end %>
</ul>
</div>
<p class="text-center">
  <%= link_to "Create a list", new_list_path(@list)%>
</p>
<div class="d-flex justify-content-center">
  <%= cl_image_tag("https://res.cloudinary.com/dqa4isfmn/image/upload/v1621602309/ztznsjviiqdxuzkjynfk.jpg",
  width: 400, height: 300, crop: :fill) %>
</div>


